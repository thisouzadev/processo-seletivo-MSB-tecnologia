{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard",
        "prettier",
        "prettier/prettier",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "space-before-function-paren":"off"
    }
}
